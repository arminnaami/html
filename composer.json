{
    "name":"netzmacht/html",
    "description":"Simple library for html rendering",
    "keywords":["Html", "helper"],
    "type":"library",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Project leader"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/html/issues",
